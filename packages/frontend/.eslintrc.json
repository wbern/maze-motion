{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "react-app",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true,
        "ecmaFeatures": {
            "modules": true,
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "prefer-const": "error"
    }
}
